<header>
        <h2>Codeial</h2>
        <ul>
            <% if (locals.user) {%>
            <li><a href="/users/profile"><%= user.name%></a></li>
            <li>
                <a href="/users/sign-out">Sign Out</a>
            </li>
            <a href="/users/profile/<%=user.id %>"><img src="<%= user.avatar %>"  width="40px" height="40px" style="border-radius: 50%; position:relative; top: 10px;"></a>
        <% }else{ %>

            <li>
                <a href="/users/sign-in">Sign In</a>
            </li>

            <li>
                <a href="/users/sign-up">Sign Up</a>
            </li>
            <% } %>
        </ul>
</header>